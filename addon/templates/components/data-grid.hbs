<table>
  {{#if showHeader}}
    <thead>
      <tr>
        {{#each propertiesList as |property|}}
          <th>{{property.label}}</th>
        {{/each}}
      </tr>
    </thead>
  {{/if}}

  <tbody>
    {{#each pageContent as |item|}}
    <tr>
        {{#each propertiesList as |property|}}
          {{data-col key=property.key item=item}}
        {{/each}}
    </tr>
    {{/each}}
  </tbody>
</table>

{{component "page-picker-paginator" paginator=paginator}}
